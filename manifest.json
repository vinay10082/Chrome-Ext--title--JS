{
    "name": "POP Title",
    "description": "Extension popups the current tab title",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions":[
        "activeTab",
        "scripting"
    ],
    "action":{
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "/images/icon_16.png",
        "32": "/images/icon_32.png",
        "48": "/images/icon_48.png"
    }
}